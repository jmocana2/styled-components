{
    "extends": [
        "eslint-config-a3media"
    ],
    "plugins": [],
    "rules": {
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": [
                    "**/stories.js",
                    "**/*.test.js",
                    "**/testUtils.js"
                ]
            }
        ],
        "react/require-default-props": 0,
        "no-unused-expressions": 0,
        "jsx-a11y/no-static-element-interactions": 0
    },
    "env": {
        "jest": true
    }
}